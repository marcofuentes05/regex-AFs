COMPILER ArchivoPrueba2

CHARACTERS 

letra = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ".
digito = "01234567890".
digitoHex = digito + "ABCDEF".
// letra0 = letra - "a".
tab = CHR(9).

// KEYWORDS CHARACTERS COMPILER  */

KEYWORDS

si = "if".
SI = "IF".
para = "for".

TOKENS
identificador = letra{letra|digito} EXCEPT KEYWORDS.
numero = digito{digito}.
numeroHex = digitoHex{digitoHex}"H".
tabulador = tab.

END ArchivoPrueba2